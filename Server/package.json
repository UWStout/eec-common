{
  "name": "karuna-server",
  "version": "0.1.0",
  "description": "Backend server / RESTful API for the Karuna communication tool.",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon --watch src src/server.js dev",
    "build-deps": "bash copyDeps.sh",
    "test": "mocha"
  },
  "license": "MIT",
  "engines": {
    "node": "14.x"
  },
  "private": true,
  "nodemonConfig": {
    "env": {
      "DEBUG": "karuna:*",
      "DEBUG_HIDE_DATE": "true",
      "DEBUG_COLORS": "true"
    }
  },
  "devDependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.5.3",
    "chai": "^4.3.4",
    "eslint": "^7.8.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eventemitter3": "^4.0.7",
    "jquery": "^3.5.1",
    "js-cookie": "^2.2.1",
    "mocha": "^8.4.0",
    "nodemon": "^2.0.7",
    "socket.io": "^3.0.0",
    "store2": "^2.12.0",
    "tiny-markdown-editor": "0.1.2"
  },
  "dependencies": {
    "aws4": "^1.11.0",
    "bcrypt": "^5.0.0",
    "chokidar": "^3.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "4.2.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "handlebars": "^4.7.7",
    "ibm-watson": "^6.1.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongodb": "3.6.3",
    "mongodb-client-encryption": "1.2.6",
    "morgan": "1.10.0",
    "supports-color": "^9.0.1",
    "saslprep": "^1.0.3"
  }
}
