{
  "name": "karuna-server",
  "version": "0.6.3",
  "description": "Backend server / RESTful API for the Karuna communication tool.",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon --watch src src/server.js dev",
    "build": "run-s copydeps client",
    "copydeps": "bash copyDeps.sh",
    "preclient": "rm -f ./public/*.js ./public/*.map ./views/**/*.js ./views/**/*.map",
    "client": "node buildClient.js",
    "clientdev": "nodemon -e js,jsx,json --watch client -- buildClient.js dev",
    "test": "mocha"
  },
  "license": "MIT",
  "engines": {
    "node": "14.x",
    "npm": "7.x"
  },
  "private": true,
  "nodemonConfig": {
    "env": {
      "DEBUG": "karuna:server:*",
      "DEBUG_HIDE_DATE": "true",
      "DEBUG_COLORS": "true"
    }
  },
  "devDependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@uiw/react-md-editor": "^3.6.4",
    "axios": "^1.2.5",
    "chai": "^4.3.4",
    "esbuild": "^0.13.8",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eventemitter3": "^4.0.7",
    "jquery": "^3.6.0",
    "js-cookie": "^2.2.1",
    "lorem-ipsum": "^2.0.4",
    "mocha": "^8.4.0",
    "nodemon": "^2.0.14",
    "npm-run-all": "^4.1.5",
    "passwd-strength": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "recoil": "^0.4.1",
    "store2": "^2.12.0",
    "tiny-markdown-editor": "0.1.4"
  },
  "dependencies": {
    "arrays-to-csv": "^2.0.1",
    "aws4": "^1.11.0",
    "bcrypt": "^5.0.1",
    "chokidar": "^3.5.2",
    "colors": "^1.4.0",
    "commonmark": "^0.30.0",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "handlebars": "^4.7.7",
    "ibm-watson": "^6.2.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongodb": "^4.1.3",
    "mongodb-client-encryption": "1.2.7",
    "morgan": "1.10.0",
    "nodemailer": "^6.7.0",
    "rate-limiter-flexible": "^2.3.1",
    "react-swipeable-views": "^0.14.0",
    "saslprep": "^1.0.3",
    "socket.io": "=4.2.0",
    "supports-color": "^9.0.2",
    "uid-safe": "^2.1.5",
    "uuid": "^8.3.2"
  }
}
